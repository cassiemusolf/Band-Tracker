@{
  Layout="Shared/_Layout.cshtml";
}

@if (@Model.Count == 0)
{
  <h3>No venues have been added for this band yet!</h3>
}
else
{
<h3>Venues for @Model["bands"].GetName():</h3>

@foreach (var venue in Model["bands"].GetVenues())
{
    <h3><a href="/bands/@Model["bands"].GetId()">@venue.GetName()</h3>
}
}
