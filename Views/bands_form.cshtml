@{
    Layout="Shared/_Layout.cshtml";
}
<div class="container">
<h1>Add A New Band</h1>

<form action="/bands" method="post">
<div class="form-group">
  <label for="band-name">Band Name:</label>
  <input id="band-name" name="band-name" type="text" size="35">
</div>
<div class="form-group">
  <label for="venue-id">Add A Venue:</label>
  <select name="venue-id">
      @foreach (var venue in Model)
      {
          <option value="@venue.GetId()">@venue.GetName()</option>
      }
  </select>
  </div>
  <button class="btn btn-lg btn-primary"  type="submit">Add New Band</button>
</form>

<h3><a href="/venues/new">Add A Venue</a> | <a href="/">Return Home</a> | <a href="/bands/new">Add A Band</a></h3>
</div>
